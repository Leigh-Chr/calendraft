{
	"name": "@calendraft/schemas",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./src/index.ts"
		},
		"./*": {
			"types": "./dist/*.d.ts",
			"default": "./src/*.ts"
		}
	},
	"scripts": {
		"build": "tsdown",
		"typecheck": "tsc --noEmit",
		"clean": "rm -rf dist *.tsbuildinfo"
	},
	"devDependencies": {
		"tsdown": "catalog:",
		"@calendraft/config": "workspace:*"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"zod": "catalog:"
	}
}
